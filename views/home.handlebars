<div class="p-5">
    <table class="table">
        <thead>
            <tr>
                <th>Nombre</th>
                <th>Descripción</th>
                <th>Fecha de Creación</th>
                <th>Acción</th>
            </tr>
        </thead>
        <tbody>
            {{#each clientes}}
            <tr>
                <td>{{this.nombre}}</td>
                <td>{{this.descripcion}}</td>
                <td>{{this.date}}</td>
                <td><a href="/todo-delete/{{this.id}}" class="btn btn-danger">Eliminar</a></td>
            </tr>
            {{/each}}

        </tbody>
    </table>


    <a href="/todo-create" class="btn btn-primary">Crear</a>
</div>